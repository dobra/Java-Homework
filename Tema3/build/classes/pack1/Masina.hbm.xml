<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="Masina" table="masini">
      <meta attribute="class-description">
         This class contains the employee detail. 
      </meta>
      <id name="id" type="int" column="id"/>
      <property name="nr_inmatriculare" column="nr_inmatriculare" type="java.lang.String"/>
      <property name="marca" column="marca" type="java.lang.String"/>
      <property name="anul" column="anul" type="int"/>
      <property name="culoare" column="culoare" type="int"/>
      <property name="nr_km" column="nr_km" type="int"/>
   </class>
 
</hibernate-mapping>